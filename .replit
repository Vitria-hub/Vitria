externalPort = 3002

[[ports]]
localPort = 36055
externalPort = 3001

[[ports]]
localPort = 37687
externalPort = 4200

[[ports]]
localPort = 39313
externalPort = 3003

[[ports]]
localPort = 40853
externalPort = 3000

[[ports]]
localPort = 42469
externalPort = 5000

[deployment]
deploymentTarget = "autoscale"
run = ["npm", "start"]
build = ["npm", "run", "build"]
